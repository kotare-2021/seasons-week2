<link rel="stylesheet" href="/main.css">
<h1 class="title">Gallery</h1>
<div class="container">
  {{#each gallery}}
  <div class="gallery-img">
    <a href="/gallery"><img class="img" src="{{image}}" alt="{{name}}"></a>
    <div class="middle">
        <div class="text-img">{{comment}}</div>
    </div>
  </div>
  {{/each}}
</div>